## In a protected file
[keystore]
path=src/it/resources/internal/testkeystore.jck
password=${env:KS_PASSWD}

[truststore]
path=src/it/resources/internal/testkeystore.jck
password=${env:KS_PASSWD}
type=JKS

[master-key]
cert.file=src/it/resources/internal/seed.crt
password=${env:KEY_PASSWD}
alias=seed

[ssl]
keystore.path=
keystore.password=
key.alias=
key.password=
